{
  "name": "demeters-law-e2e",
  "version": "1.0.0",
  "description": "The e2e testing suite for Demeter's Law",
  "scripts": {
    "build": "rimraf dist && tsc || echo 'built, with errors'",
    "build:tsc": "tsc",
    "ci": "start-server-and-test http://localhost:3001 test",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:edge": "cypress run --browser edge",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:parallel": "cypress run --parallel",
    "lint": "eslint '**/*.{js,ts}' --max-warnings=0",
    "spellcheck": "cspell \"**/*.*\" --no-progress",
    "start": "http-server -c-1 --silent -p 3001",
    "test": "start-server-and-test start http://localhost:3030 cy:run",
    "validate": "npm run spellcheck && npm run build:tsc && npm run lint && npm run test"
  },
  "author": "Houston Green",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "babel-loader": "^9.1.3",
    "cspell": "^7.2.0",
    "cypress": "^13.0.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  }
}
